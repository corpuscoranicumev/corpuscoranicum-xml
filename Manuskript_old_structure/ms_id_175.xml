                    <pc type="kufi">۝</pc>
                    <w n="000000000">ڡالَ</w>
                    <w n="000000000">أَراعٮ ٌ أٮٮَ</w>
                    <w n="000000000">عں</w>
                    <w n="000000000">آلهٮى</w>
                    <w n="000000000">ٮإِٮرهٮمُ لَئِں</w>
                    <w n="000000000">لَم</w>
                    <w n="000000000">ٮٮٮهِ</w>
                    <w n="000000000">لَأَرحمٮَكَ وَاهحرٮى</w>
                    <w n="000000000">مَلٮًا</w>
                    <pc type="kufi">۝</pc>
                    <w n="000000000">ڡالَ</w>
                    <w n="000000000">سلمٌ علٮكَ</w>
                    <w n="000000000">سأَسٮعڡرُ</w>
                    <w n="000000000">لَكَ</w>
                    <w n="000000000">رٮى</w>
                    <w n="000000000">إِٮَهُ</w>
                    <w n="000000000">كانَ</w>
                    <w n="000000000">ٮى</w>
                    <w n="000000000">حڡٮًا</w>
                    <pc type="kufi">۝</pc>
                    <w n="000000000"> وأَعٮرلُكم</w>
                    <w n="000000000">وما ٮدعونَ</w>
                    <w n="000000000">من</w>
                    <w n="000000000">دونِ</w>
                    <w n="000000000">اللهِ</w>
                    <w n="000000000">وأَدعوا</w>
                    <w n="000000000">رٮى</w>
                    <w n="000000000">عسى أَلا</w>
                    <w n="000000000">أَكونَ</w>
                    <w n="000000000">ٮدعاِ رٮى</w>
                    <w n="000000000">سڡٮًا</w>
                    <pc type="kufi">۝</pc>
                    <w n="000000000">ڡلمَا</w>
                    <w n="000000000">اعٮرلهُم وما</w>
                    <w n="000000000">ٮعٮدونَ</w>
                    <w n="000000000">من</w>
                    <w n="000000000">دونِ</w>
                </p>
